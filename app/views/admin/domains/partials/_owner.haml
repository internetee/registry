.panel.panel-default
  .panel-heading
    %h3.panel-title= t('.title')
  .panel-body
    %dl.dl-horizontal
      %dt= t(:name)
      %dd= link_to(@domain.registrant, [:admin, @domain.registrant])

      %dt= t(:id)
      %dd= @domain.registrant_code

      %dt= t(:identity_code)
      %dd= @domain.registrant_ident

      %dt= t(:email)
      %dd= @domain.registrant_email

      %dt= t(:phone)
      %dd= @domain.registrant_phone
